  <tainingAndEducation_...n>training_...n</trainingAndEducation_...n>
  <trainingYear_1><trainingYear_1></trainingYear_1>
  <trainingYear_..n>trainingYear_..n</trainingYear_..n>
 </trainingAndEducation>
 <knownLanguages>
  <knownLanguage_1>
